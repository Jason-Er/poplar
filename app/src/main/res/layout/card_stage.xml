<?xml version="1.0" encoding="utf-8"?>
<com.wecyberstage.wecyberstage.view.compose.StageCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/card_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:ratio="1.78">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/stage_view_setting"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:layout_weight="618"
            android:adjustViewBounds="false"
            android:background="@mipmap/ic_launcher"></ImageView>

        <ImageView
            android:id="@+id/stage_view_floor"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="center"
            android:layout_weight="382"
            android:background="@mipmap/ic_launcher"></ImageView>
    </LinearLayout>

</com.wecyberstage.wecyberstage.view.compose.StageCardView>