<?xml version="1.0" encoding="utf-8"?>
<com.wecyberstage.wecyberstage.view.main.MaskChooseTabLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_behavior=".view.main.MaskChooseBehavior">

    <android.support.v4.view.ViewPager
        android:id="@+id/maskChooseTabLayout_viewPager"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

    </android.support.v4.view.ViewPager>

    <android.support.design.widget.TabLayout
        android:id="@+id/maskChooseTabLayout_tabLayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_56"
        android:background="@color/white">

    </android.support.design.widget.TabLayout>

</com.wecyberstage.wecyberstage.view.main.MaskChooseTabLayout>